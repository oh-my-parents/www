{
  "env": { "browser": true, "es2020": true },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "import"],
  "extends": ["naver","prettier"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "semi":"off",
    "no-unused-vars": "off",
    "linebreak-style": "off"
  }
}
